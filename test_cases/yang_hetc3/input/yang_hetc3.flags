--max_score0sym 5.0
--dbfiles 
   database/hetc3.json
   input/local_HFuse_het_2chain_2arm_database.Sh13-5+1-AI_2.20180905.txt
   input/local_HFuse_het_2chain_2arm_database.ZCON-9_2.20180516.txt

